    "n,x=map(int,input().split( ))\n",
    "x_i=[]\n",
    "x_i+=map(int,input().split( ))\n",
    "x_i+=[x]\n",
    "x_i.sort()\n",
    "x_c=[]\n",
    "\n",
    "    \n",
    "    \n",
    "for i in range(len(x_i)-1):\n",
    "    x_c+=[x_i[i+1]-x_i[i]]\n",
    "\n",
    "def fun(x):\n",
    "    return x%min(x_c)\n",
    "  \n",
    "li=list(map(fun,x_c))\n",
    "\n",
    "if max(li)==0:\n",
    "    print(min(x_c))\n",
    "else:\n",
    "    print(1)"
